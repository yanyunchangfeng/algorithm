{
  "name": "yycf-algorithm",
  "version": "12.0.0",
  "description": "1.冒泡算法 2.快速排序 3.插入排序,4数组去重,5.质数判定,6.日历算法,7.颜色轮训算法,8.类的多重继承 9.生成随机数",
  "main": "index.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./bubble-sort": {
      "types": "./dist/types/utils/bubble-sort/index.d.ts",
      "node": "./dist/cjs/utils/bubble-sort/index.js",
      "require": "./dist/cjs/utils/bubble-sort/index.js",
      "es2015": "./dist/esm/utils/bubble-sort/index.js",
      "default": "./dist/esm5/utils/bubble-sort/index.js"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "start-analyzer": "cross-env NODE_ENV=development ANALYZE=1  webpack serve",
    "start-unused": "cross-env NODE_ENV=development UNUSED=1  webpack serve",
    "start-multi-page": "cross-env NODE_ENV=development MULTIPLE=1  webpack serve",
    "start-multi-page-unused": "cross-env NODE_ENV=development MULTIPLE=1 UNUSED=1  webpack serve",
    "build-analyzer": "cross-env NODE_ENV=production ANALYZE=1  webpack",
    "build-multi-page": "cross-env NODE_ENV=production MULTIPLE=1  webpack",
    "build-multi-page-analyzer": "cross-env NODE_ENV=production MULTIPLE=1 ANALYZE=1  webpack",
    "generateAnalyzerFile": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "analyzer": "webpack-bundle-analyzer --port 8888 ./docs/stats.json",
    "clean": "rimraf ./dist",
    "build-css": "node-sass ./src/index.scss ./dist/index.css",
    "build": "npm run clean && npm run build-ts",
    "build-ts": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "build:umd": "cross-env NODE_ENV=production webpack build",
    "build:esm": "tsc -p tsconfig.json --declaration --declarationDir dist/types -m es6 --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.json -m commonjs --outDir dist/cjs",
    "build-umd-esm-cjs": "npm run clean && npm run build:umd && npm run build:esm && npm run build:cjs"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/webpack-env": "1.16.3",
    "copy-webpack-plugin": "10.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.1.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "4.1.0",
    "html-minimizer-webpack-plugin": "4.1.0",
    "html-webpack-plugin": "5.5.0",
    "image-webpack-loader": "8.1.0",
    "mini-css-extract-plugin": "2.6.0",
    "sass": "1.49.11",
    "sass-loader": "12.6.0",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "thread-loader": "3.0.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "unused-webpack-plugin": "2.4.0",
    "webpack": "5.71.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.8.0",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yanyunchangfeng/algorithm.git"
  },
  "keywords": [
    "yycf",
    "algorithm"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "yanyunchangfeng <yanyunchangfeng@gmail.com> (https://github.com/yanyunchangfeng)",
  "license": "MIT"
}
